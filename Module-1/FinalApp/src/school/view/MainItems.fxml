<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../../myStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="school.view.MainItemsController">
   <center>
      <FlowPane hgap="20.0" prefHeight="200.0" prefWidth="200.0" vgap="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#goTeachers" prefHeight="100.0" prefWidth="120.0" text="Teacher Records" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#goStudents" prefHeight="100.0" prefWidth="120.0" text="Student Records" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#goJanitors" prefHeight="100.0" prefWidth="120.0" text="Janitor Records" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button layoutX="310.0" layoutY="30.0" mnemonicParsing="false" onAction="#goDatabase" prefHeight="100.0" prefWidth="120.0" text="Database" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button layoutX="450.0" layoutY="30.0" mnemonicParsing="false" onAction="#goView" prefHeight="100.0" prefWidth="120.0" text="View" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets left="20.0" top="20.0" />
         </padding>
      </FlowPane>
   </center>
</BorderPane>
